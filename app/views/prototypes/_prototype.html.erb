<div class="card">
  
  <%=link_to image_tag prototype.image %>
  <div class="card__body">
    <%=link_to prototype.title, prototype_path(prototype.id),method: :get  %>
    <p class="card__summary">
      <%= link_to prototype.catch_copy,prototype_path(prototype.id),method: :get %>
    </p>
    <%= link_to prototype.user.name,prototype_path(prototype.id),method: :get %>
  </div>
</div>